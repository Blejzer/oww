    <form class="form-horizontal" ng-controller="MyCtrl as up" name="up.upload_form">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><strong>Separated</strong> Layout</h3>
                <ul class="panel-controls">
                    <li><a href="#" class="panel-remove"><span class="fa fa-times"></span></a></li>
                </ul>
            </div>
            <div class="panel-body">
                <p>Add class <code>.form-group-separated</code> to form wrapper to get rows separated</p>
            </div>
            <div class="panel-body form-group-separated">

                <div class="form-group">
                    <label class="col-md-3 col-xs-12 control-label">Title of the event</label>
                    <div class="col-md-6 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                            <input ng-model="up.title" id="title" name="title" class="form-control" autocomplete="off"
                                   pattern="^([\Sa-zA-Z]){1,25}$"
                                   oninvalid="setCustomValidity('Plz enter only English Alphabet\n ' +
                                                                'No spaces, numbers, or funny characters allowed!')"
                                   onchange="try{setCustomValidity('')}catch(e){}" required/>
                        </div>
                        <span class="help-block">This is sample of text field</span>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-3 col-xs-12 control-label">Image preview</label>
                    <div class="col-md-6 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon"><span class="fa fa-unlock-alt"></span></span>
                            <img style="max-height:640px;" ngf-src="up.file || 'images/wowMarine.png'" class="ngf-hide" />
                        </div>
                        <span class="help-block">Password field sample</span>
                    </div>
                </div>

                <!--<div class="form-group">-->
                    <!--<form ng-submit="submit1()" ng-controller="AppCtrl" ng-cloak="">-->
                        <!--<div class="form-group">-->
                            <!--<label class="col-md-3 col-xs-12 control-label"><p>Selected Value: <span>{{ orientation }}</span> </p></label>-->
                            <!--<div class="col-md-6 col-xs-12">-->
                                <!--<div class="input-group radio">-->
                                    <!--<span class="input-group-addon"><span class="fa fa-unlock-alt"></span></span>-->
                                    <!--<label>-->
                                        <!--<input type="checkbox" ng-model="value" value={{orientation}}>-->
                                        <!--Value = FOO-->
                                    <!--</label>-->
                                <!--</div>-->
                                <!--<span class="help-block">Password field sample</span>-->
                            <!--</div>-->
                        <!--</div>-->

                    <!--</form>-->
                <!--</div>-->


                <div class="form-group">
                    <label class="col-md-3 col-xs-12 control-label">Datepicker</label>
                    <div class="col-md-6 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon"><span class="fa fa-calendar"></span></span>
                            <i ng-show="up.upload_form.file.$error.required">*required</i><br>
                            <i ng-show="up.upload_form.file.$error.maxSize">File too large
                                {{up.file.size / 1000000|number:1}}MB: max 20M</i>
                        </div>
                        <span class="help-block">Click on input field to get datepicker</span>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-3 col-xs-12 control-label">Progress</label>
                    <div class="col-md-6 col-xs-12">
                        <p>{{up.progress}}</p>
                        <span class="help-block">Upload progress</span>
                    </div>
                </div>

                <!--<div class="form-group">-->
                    <!--<label class="col-md-3 col-xs-12 control-label">Tags</label>-->
                    <!--<div class="col-md-6 col-xs-12">-->
                        <!--<input type="text" class="tagsinput" value="First,Second,Third"/>-->
                        <!--<span class="help-block">Default textarea field</span>-->
                    <!--</div>-->
                <!--</div>-->

                <div class="form-group">
                    <label class="col-md-3 col-xs-12 control-label">Select the week you want to set!</label>
                    <div class="col-md-6 col-xs-12">
                        <select class="form-control select">
                            <option>Week x 1</option>
                            <option>Week x 2</option>
                            <option>Week x 3</option>
                            <option>Week x 4</option>
                            <option>Week x 5</option>
                        </select>
                        <span class="help-block">Select box example</span>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-3 col-xs-12 control-label">File</label>
                    <div class="col-md-6 col-xs-12">
                        <input
                                type="file"
                                ngf-select
                                ng-model="up.file"
                                name="file"
                                ngf-pattern="'image/*'"
                                accept="image/*"
                                ngf-max-size="20MB"
                                class="fileinput btn-primary"
                        />
                        <span class="help-block">Input type file</span>
                    </div>
                </div>

                <!--<div class="form-group">-->
                    <!--<label class="col-md-3 col-xs-12 control-label">Checkbox</label>-->
