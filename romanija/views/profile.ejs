                        <!--<span class="help-block">Checkbox sample, easy to use</span>-->
                    <!--</div>-->
                <!--</div>-->

            </div>
            <div class="panel-footer">
                <button class="btn btn-default">Clear Form</button>
                <button class="btn btn-primary pull-right" ng-click="up.submit()">Submit</button>
            </div>
        </div>
    </form>
