                    <div class="profile-image">
                        <img src="joli/assets/images/users/user0.jpg" alt="John Doe"/>
                    </div>
                    <div class="profile-data">
                        <div class="profile-data-name"><%= user.username%></div>
                        <div class="profile-data-title">World's Word Administrator</div>
                    </div>
                </div>
            </li>
            <li class="xn-title">Navigation</li>
            <li>
                <a href="main"><span class="fa fa-desktop"></span> <span class="xn-text">Dashboard</span></a>
            </li>
            <li class="active">
                <a href="event"><span class="fa fa-bullhorn"></span> Add new event</a>
            </li>
            <li>
                <a href="person"><span class="fa fa-users"></span> Add new person</a>
            </li>
        </ul>
        <!-- END X-NAVIGATION -->
    </div>
    <!-- END PAGE SIDEBAR -->

    <!-- PAGE CONTENT -->
    <div class="page-content">

        <!-- START X-NAVIGATION VERTICAL -->
        <ul class="x-navigation x-navigation-horizontal x-navigation-panel">
            <!-- TOGGLE NAVIGATION -->
            <li class="xn-icon-button">
                <a href="#" class="x-navigation-minimize"><span class="fa fa-dedent"></span></a>
            </li>
            <!-- END TOGGLE NAVIGATION -->
            <!-- SEARCH -->
            <!--<li class="xn-search">-->
                <!--<form role="form">-->
                    <!--<input type="text" name="search" placeholder="Search..."/>-->
                <!--</form>-->
            <!--</li>-->
            <!-- END SEARCH -->
            <!-- SIGN OUT -->
            <li class="xn-icon-button pull-right">
                <a href="/logout" class="mb-control" data-box="#mb-signout"><span class="fa fa-sign-out"></span></a>
            </li>
            <!-- END SIGN OUT -->

        </ul>
        <!-- END X-NAVIGATION VERTICAL -->

        <!-- START BREADCRUMB -->
        <ul class="breadcrumb">
            <li><a href="#">Home</a></li>
            <li class="active">Dashboard</li>
        </ul>
        <!-- END BREADCRUMB -->

        <!-- PAGE CONTENT WRAPPER -->
        <div class="page-content-wrap">

            <!-- START WIDGETS -->
            <div class="row">
                <div class="col-md-10">



                    <div>
                        <h1>Event File Upload</h1>

                        <form class="form-horizontal" ng-controller="MyCtrl as up" name="up.upload_form">
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h3 class="panel-title"><strong>Event</strong> for a week</h3>
                                    <ul class="panel-controls">
                                        <li><a href="#" class="panel-remove"><span class="fa fa-times"></span></a></li>
                                    </ul>
                                </div>
                                <div class="panel-body">
                                    <p>Add new <code>EVENT</code> for the week you select. </p>
                                </div>
                                <div class="panel-body form-group-separated">

                                    <div class="form-group">
                                        <label class="col-md-3 col-xs-12 control-label">Title of the event</label>
                                        <div class="col-md-6 col-xs-12">
                                            <div class="input-group">
                                                <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                                <input ng-model="up.title" id="title" name="title" class="form-control" autocomplete="off" required/>
                                            </div>
                                            <span class="help-block">Please enter the title of the event</span>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-md-3 col-xs-12 control-label">Select the week you want to set!</label>

                                        <div class="col-md-6 col-xs-12 input-group date">
                                            <input class="form-control datepicker"  ng-model="up.week" name="week" id="week" type="text"
                                                   placeholder="YYYY-W##" min="2017W6"
                                                   max="2017W52" required />
                                            <span class="input-group-addon add-on"><span class="glyphicon glyphicon-calendar"></span></span>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-md-3 col-xs-12 control-label">Image preview</label>
                                        <div class="col-md-6 col-xs-12">
                                            <div class="input-group">
                                                <span class="input-group-addon"><span class="fa fa-picture-o"></span></span>
                                                <img style="max-height:640px;" ngf-src="up.file || 'images/wowMarine.png'" class="ngf-hide" />
                                                <span class="input-group-addon"><span class="fa fa-picture-o"></span></span>
                                            </div>
                                            <span class="help-block">Image file preview must be visible</span>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 col-xs-12 control-label">Progress</label>
                                        <div class="col-md-6 col-xs-12 input-group">
                                            <div class="progress-bar progress-bar-success progress-bar-striped active"  role="progressbar" aria-valuenow="{{up.bar}}" aria-valuemin="0" aria-valuemax="100" style="width: {{up.bar}}%">{{up.progress}}</div>
                                        </div>
                                    </div>



                                    <div class="form-group">
                                        <label class="col-md-3 col-xs-12 control-label">File</label>
                                        <div class="col-md-6 col-xs-12">
                                            <div class="input-group">
                                                <span class="input-group-addon"><span class="fa fa-cloud-upload"></span></span>
                                                <input
                                                        type="file"
                                                        ngf-select
                                                        ng-model="up.file"
                                                        name="file"
                                                        ngf-pattern="'image/*'"
                                                        accept="image/*"
                                                        ngf-max-size="20MB"
                                                        class="fileinput btn btn-primary"
                                                />
                                            </div>
                                            <span class="help-block">Only accepting IMAGE file types</span>
                                        </div>
                                    </div>

                                </div>
                                <div class="panel-footer">
                                    <button class="btn btn-default">Clear Form</button>
                                    <button class="btn btn-primary pull-right" ng-click="up.submitEvent()">Submit</button>
                                </div>
                            </div>
                        </form>
                    </div>









                </div>

                <div class="col-md-2">

                    <!-- START WIDGET CLOCK -->
                    <div class="widget widget-info widget-padding-sm">
                        <div class="widget-big-int plugin-clock">00:00</div>
                        <div class="widget-subtitle plugin-date">Loading...</div>
                        <div class="widget-controls">
                            <a href="#" class="widget-control-right widget-remove" data-toggle="tooltip" data-placement="left" title="Remove Widget"><span class="fa fa-times"></span></a>
                        </div>
                        <div class="widget-buttons widget-c3">
                            <div class="col">
                                <a href="#"><span class="fa fa-clock-o"></span></a>
                            </div>
                            <div class="col">
                                <a href="#"><span class="fa fa-bell"></span></a>
                            </div>
                            <div class="col">
                                <a href="#"><span class="fa fa-calendar"></span></a>
                            </div>
                        </div>
                    </div>
                    <!-- END WIDGET CLOCK -->

                </div>
            </div>
            <!-- END WIDGETS -->


        </div>
        <!-- END PAGE CONTENT WRAPPER -->
    </div>
    <!-- END PAGE CONTENT -->
</div>
